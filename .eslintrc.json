{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "standard-with-typescript"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "quote-props": 0,
    "quotes": 0,
    "comma-dangle": 0,
    "object-curly-spacing": 0,
    "semi": 0,
    "space-before-function-paren": 0
  }
}
